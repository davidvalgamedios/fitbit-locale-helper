{
  "name": "fitbit-locale-helper",
  "version": "0.1.2",
  "description": "This library allows you to easily generate locale (.po files) for your Fitbit app or clockface",
  "main": "bin/index.js",
  "scripts": {
    "build": "rm -r bin && tsc",
    "lint": "eslint src/**/*.ts",
    "dev": "tsc && node bin/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage=true && (sensible-browser coverage/lcov-report/index.html || open coverage/lcov-report/index.html)"
  },
  "bin": {
    "fitbit-locale-generate": "bin/index.js"
  },
  "author": "Valga Medios",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.16.1"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "fitbitdev",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidvalgamedios/fitbit-locale-helper.git"
  },
  "bugs": {
    "url": "https://github.com/davidvalgamedios/fitbit-locale-helper/issues"
  },
  "homepage": "https://github.com/davidvalgamedios/fitbit-locale-helper#readme"
}
